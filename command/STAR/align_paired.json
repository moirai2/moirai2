{"https://moirai2.github.io/schema/daemon/input":["$input1","$input2","$starindex"],"https://moirai2.github.io/schema/daemon/bash":["if [[ $input1 == *.gz ]]","then","filename=$(basename $input1 .gz)","gzip -cd $input1 > $tmpdir/read1.fq","gzip -cd $input2 > $tmpdir/read2.fq","elif [[ $input1 == *.bz2 ]]","then","filename=$(basename $input1 .bz2)","bzip2 -cd $input1 > $tmpdir/read1.fq","bzip2 -cd $input2 > $tmpdir/read2.fq","else","filename=$(basename $input1)","cp $input1 $tmpdir/read1.fq","cp $input2 $tmpdir/read2.fq","fi","basename=\"${filename%.*}\"","sam=\"$tmpdir/$basename.Aligned.out.sam\"","tmp=\"$tmpdir/tmp.bam\"","bam=\"$tmpdir/$basename.bam\"","log=\"$tmpdir/$basename.Log.final.out\"","STAR --genomeDir $starindex --readFilesIn $tmpdir/read1.fq $tmpdir/read2.fq --outFileNamePrefix $tmpdir/$basename. --outSAMattributes All --outSAMunmapped Within","rm $tmpdir/read1.fq","rm $tmpdir/read2.fq","samtools view -bSo $tmp $sam","rm $sam","samtools sort -o $bam $tmp 2>/dev/null","rm $tmp","rm $tmpdir/$basename.Log.out","rm $tmpdir/$basename.Log.progress.out","rm $tmpdir/$basename.SJ.out.tab","rmdir $tmpdir/*._STARtmp/ > /dev/null 2>&1"],"https://moirai2.github.io/schema/daemon/output":["$bam","$log"],"https://moirai2.github.io/schema/daemon/error/stdout/ignore":"finished successfully"}
